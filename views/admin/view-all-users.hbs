<section>
    <div class="container" style="width: 61%;
    margin-left: 26%;
    margin-top: 3%;">
        <div class="row mt-4">
            <a href="/admin/add-users" class="btn btn-success ml-auto">Add User</a>
           
        </div>
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th> 
      <th scope="col">Mobile</th> 
      <th scope="col">Status</th> 
     
      
    </tr>
  </thead>
  <tbody>
   {{#each users}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.Name}}</td>
      <td>{{this.Email}}</td>
      <td>{{this.Mobile}}</td>
      <td>{{this.Status}}</td>
     
     
      <td>
         <a href="/admin/edit-user/{{this._id}}" class="btn btn-primary">Edit</a>
        
      </td>
      </tr>
   {{/each}}
  </tbody>
</table>
    </div>
</section>