<!-- markets -->
<div class="markets" id="markets">
	<div class="container">
		<div class="wthree-heading">
			<h3>PRODUCTS</h3>
		</div>
		<div class="markets-grids">

			<section>
				<div class="container mt-5">
					<div class="row">
						{{#each products}}

						<div class="col-md-3">

							<div class="card" style="width: 18rem;">
								<img  style="width: 50px;height:50px" class="card-img-top" src="/product-images/{{this._id}}.jpg" alt="Card image cap">
								<div class="card-body">
									<h5 class="card-title">{{this.Productname}}</h5>
									<p class="card-text">{{this.Category}}</p>
									<h5 class="card-title">{{this.Price}}</h5>
									<a href="/add-to-cart/{{this._id}}" class="btn btn-primary">Add to Cart</a>
									

								</div>
							</div>


						</div>
						

						{{/each}}
					</div>
				</div>
			</section>



		</div>
	</div>
</div>
